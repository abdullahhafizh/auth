{
    "name": "abdullahhafizh/auth",
    "description": "Auth middleware",
    "keywords": [
        "abdullah",
        "hafizh"
    ],
    "homepage": "https://github.com/abdullahhafizh/auth",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdullah Hafizh",
            "email": "abdllhhafizh@gmail.com",
            "homepage": "https://hifiez.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.1|^6.0",
        "illuminate/http": "^5.1|^6.0",
        "firebase/php-jwt": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3.5",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "classmap": [
            "app/controllers"
        ],
        "psr-4": {
            "Abdullahhafizh\\Auth\\": "src",
            "app-controllers\\" : "app/controllers"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "post-autoload-dump": [
            "@php -r \"rename('src/Controllers/Controller.php', 'app-controllers/Controller.php');\""
        ]
    }
}
